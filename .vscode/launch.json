    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch fbus tests",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/FBus.Tests/bin/Debug/netcoreapp3.1/FBus.Tests.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Launch generichost tests",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/FBus.GenericHost.Tests/bin/Debug/netcoreapp3.1/FBus.GenericHost.Tests.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "launch client",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/samples/client/bin/Debug/netcoreapp3.1/client.dll",
            "args": ["server1"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "launch server",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/samples/server-console/bin/Debug/netstandard2.1/server-console.dll",
            "args": [],
